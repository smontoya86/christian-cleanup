<svg width="300" height="300" viewBox="0 0 300 300" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="300" height="300" fill="#e9ecef" stroke="#dee2e6" stroke-width="2"/>
  
  <!-- Playlist icon -->
  <g transform="translate(150, 150)">
    <!-- List lines -->
    <rect x="-60" y="-40" width="80" height="8" rx="4" fill="#495057"/>
    <rect x="-60" y="-20" width="60" height="8" rx="4" fill="#495057"/>
    <rect x="-60" y="0" width="70" height="8" rx="4" fill="#495057"/>
    <rect x="-60" y="20" width="50" height="8" rx="4" fill="#495057"/>
    <rect x="-60" y="40" width="75" height="8" rx="4" fill="#495057"/>
    
    <!-- Play button -->
    <circle cx="30" cy="0" r="25" fill="#007bff" stroke="#0056b3" stroke-width="2"/>
    <polygon points="22,-10 22,10 38,0" fill="white"/>
  </g>
  
  <!-- Text -->
  <text x="150" y="250" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#495057">Playlist</text>
</svg> 